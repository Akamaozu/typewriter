{
  "name": "reasonml",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "reason-scripts": "1.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "typewriter-remote": "node ../../../dist/src/index.js gen-js --trackingPlanId {YOUR_TRACKING_PLAN_ID} --workspaceSlug {YOUR_WORKSPACE_SLUG} --token {YOUR_API_TOKEN} --outputPath ./pages/generated --declarations reasonml",
    "typewriter-local": "node ../../../dist/src/index.js gen-js --inputPath ../../local-tracking-plans/tracking-plan-web.json --outputPath ./src/analytics/generated --declarations reasonml",
    "generate": "yarn run typewriter-local"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@glennsl/bs-jest": "^0.4.5",
    "bs-fetch": "^0.3.1",
    "bs-platform": "^4.0.7",
    "reason-react": "^0.5.3"
  }
}
